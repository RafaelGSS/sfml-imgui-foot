!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.9~svn20110310	//
CATCH_CONFIG_MAIN	test/catch_main.cpp	1;"	d	file:
CXX	Makefile	/^CXX ?= g++$/;"	m
DEPS	Makefile	/^DEPS := $(OBJS:.o=.d)$/;"	m
FLAGS	Makefile	/^FLAGS ?= $(INC_FLAGS) -MMD -MP$/;"	m
Factorial	test/constexpr_tests.cpp	/^constexpr unsigned int Factorial(unsigned int number)$/;"	f
Factorial	test/tests.cpp	/^unsigned int Factorial(unsigned int number)$/;"	f
INC_DIRS	Makefile	/^INC_DIRS := $(shell find $(SRC_DIRS) -type d)$/;"	m
INC_FLAGS	Makefile	/^INC_FLAGS := $(addprefix -I,$(INC_DIRS))$/;"	m
LDFLAGS	Makefile	/^LDFLAGS = -Wall$/;"	m
LDLIBS	Makefile	/^LDLIBS = -lsfml-graphics -lsfml-window -lsfml-system$/;"	m
LLVMFuzzerTestOneInput	fuzz_test/fuzz_tester.cpp	/^extern "C" int LLVMFuzzerTestOneInput(const uint8_t *Data, size_t Size)$/;"	f
OBJS	Makefile	/^OBJS := $(addsuffix .o,$(basename $(SRCS)))$/;"	m
SRCS	Makefile	/^SRCS := $(shell find $(SRC_DIRS) -name "*.cpp" -or -name "*.c" -or -name "*.s")$/;"	m
SRC_DIRS	Makefile	/^SRC_DIRS ?= .\/src$/;"	m
TARGET	Makefile	/^TARGET = $(notdir $(basename $<))$/;"	m
main	src/main.cpp	/^int main()$/;"	f
sum_values	fuzz_test/fuzz_tester.cpp	/^[[nodiscard]] auto sum_values(const uint8_t *Data, size_t Size)$/;"	f
